package simple_calendar.simple_calendar;

import java.sql.Time;


public class Event {
	String name;
	Time timeOfEvent;
	String thingsToCarry;
	
	@SuppressWarnings("deprecation")
	public Event(String n, int h, int m, int s, String carry){
		name = n;
		thingsToCarry = carry;
		timeOfEvent = new Time(h, m, s);
	}
}
